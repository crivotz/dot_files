# alias rm='command rm -i'
# alias diff='bd --delta'
alias df='df -h'  du='du -h'      cp='cp -v'   mv='mv -v'      plast="last -20"

# TMUX
alias ta='tmux attach -t'
alias tks='tmux kill-session -t'
alias tl='tmux list-sessions'
alias ts='tmux new-session -s'
alias tx='tmux new -s λ'
alias lambda='ta λ'

# SHORTCUT

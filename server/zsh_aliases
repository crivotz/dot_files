# ROUTE PARTICOLARI
alias route-del-default='sudo ip r del default' 
# COMANDI ALTERNATIVI
alias :q="exit"
alias n="nvim"
alias rm="gtrash put"
alias tar-comp="_tar_comp"
alias tar-decomp="_tar_decomp"
alias ls='eza --git --icons --classify --group-directories-first --time-style=long-iso --group --color-scale'
